   <div id="bam" class="someClass">hello world</div>
   
test uids
 arrange
  require ../../products/stump.nodejs.js
  constructWithParagraph
   div hello world
    id bam
    class someClass
 getFirstParticle
  asHtmlWithSuids
   assertTypeIs string
   assertStringIncludes stumpUid
  isStumpParticleCheckbox
   assertStringIs false
  asHtml
   assertTypeIs string
  isInputType
   assertStringIs false
  getStumpParticleAttr id
   assertStringIs bam
test syntax
 arrange
  require ../../products/stump.nodejs.js
  constructWithParagraph
   div
    bern
     Hello world
 toAtomTypeParticles
  assertParagraphIs
   htmlTagNameAtom
    bernKeyatomAtom
     anyHtmlContentAtom anyHtmlContentAtom